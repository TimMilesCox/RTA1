.	AF_INET	$equ	2
.	PF_INET	$equ	2

IP	$equ	0800
ARP	$equ	0806

framet1* $proc
*	$tree
length	$res	1
to	$res	3
from	$res	3
protocol +	framet1(1, 1)
	$root
	$end

