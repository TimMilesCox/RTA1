TCP_CLOSED	$equ	1
TCP_LISTEN	$equ	2
TCP_SYNSENT	$equ	3
TCP_SYNRECEIVED	$equ	4
TCP_ESTABLISHED	$equ	5
TCP_FINWAIT1	$equ	6
TCP_FINWAIT2	$equ	7
TCP_CLOSEWAIT	$equ	8
TCP_LASTACK	$equ	9
TCP_CLOSING	$equ	10
TCP_TIMEWAIT	$equ	11
TCP_DELETETCB	$equ	12

TCP_RELINQUISHED $equ	64

TCP_URG	$equ	32
TCP_ACK	$equ	16
TCP_PSH	$equ	8
TCP_RST	$equ	4
TCP_SYN	$equ	2
TCP_FIN	$equ	1

TCP_HL		$equ	0F000
TCP_FLAGS	$equ	00FFF

TCP_5WORDH	$equ	05000
TCP_6WORDH	$equ	06000

TCP_TIMER1	$equ	10*60000
TCP_TIMER2	$equ	3000
TCP_TIMEWAIT_MS	$equ	10000
TCP_STATES_BEAT	$equ	10	. MS interval between state machine passes

p	$proc
segment* $name
*	$tree
sourcep	+	0
destp	+	0
seq	+	0:d
ack	+	0:d
hl_code	+	05010
window	+	0
checksum +	0
urgent	+	0
mss_option + 0204, 1460:d
	$root
	$end
