
f	$func
log2*	$name
fulcrum	$set	f(1, 1)-1
	$do	fulcrum<1,	$return	0
log	$do	48,	$do	1*/log>fulcrum,	$return	log
	$return	-1
	$end

log2:zero	$equ	log2(0)
log2:1		$equ	log2(1)
log2:2		$equ	log2(2)
log2:3		$equ	log2(3)
log2:15		$equ	log2(15)
log2:16		$equ	log2(16)
log2:17		$equ	log2(17)
log2:16383	$equ	log2(16383)
log2:16384	$equ	log2(16384)
log2:16385	$equ	log2(16385)

