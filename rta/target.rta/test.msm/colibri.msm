	$path		../
	$include	def/rta.def
	$include	def/ii.def
	$include	language/stack.def

COLIBRI	$equ	36*1024
LIMIT1	$equ	253

p	$proc
column$* $name

v*	$proc
"v(1, 1)_hx"***	$vector	v(1, 1)_r
	$end

w*	$proc
"w(1, 1)_hx_nocheck"*** $vector w(1, 1)_rnok
	$end

p	$proc	*
routine* $name
"p(1, 1)_r"**
	jyge	abandon
"p(1, 1)_rnok"**
	lr, xi	LIMIT1
	jyge	notsofa
	ex	p(1, 1)_table, y
	fret	0
notsofa	dpush	a
	la	y
	dsr	1
	sbr	23
	ax	a
	ly	b
	pop	a
	pop	b
	ex	p(1, 1)_table, y
	fret	0
	$end

p	$proc
table*	$name
"p(1, 1)_table"**
?	$do	LIMIT1,	p(1, 1)	+(?-1)/2, x, +(?-1)///2+h1
	$end

?	$do	p(),	v	p(?, 1)
?	$do	p(),	w	p(?, 1)
	$list	0
	$do	64-$,	fret	0
	$list
?	$do	p(),	routine	p(?, 1)
abandon* ii	RUNAWAY$
	$do	$<256,$(0:256)
	$list	0
?	$do	p(),	table	p(?, 1)
	$list
	$do	$>4095,	$flag	LIMIT1 too high
	$end

$(0::COLIBRI)
	column$	sa sb z la lb or orB and andB xor xorB aa ab ana anb

