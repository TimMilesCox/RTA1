
p	$proc
sort_out* $name
index	$equ	p()+1
?	$do	p(),	+	p(1, 2), p(index-?, 1)
	$end

p	$proc
backslang* $name
?	$do	p(),	+	p(?, 1), p(?, 3), p(?, 2)
	$end

p       $proc
yaname* $name		101
        sort_out        p(0,0), p(0, 2)	p()
        backslang       p(3) p(2) p(1)
	$end

	yaname,40	1, 2, 3		4, 5, 6		7, 8, 9

