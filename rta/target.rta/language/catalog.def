	$do	($t(__B1_FRAMES)=0)++(__B1_FRAMES<1)++(__B1_FRAMES>59),;
__B1_FRAMES*	$set	1

__OFFSET $set	050
	$do	catalog()>0,__OFFSET	$set	catalog(1, 1)

	+	__PAGES-1*/18++__OFFSET, __BASE+__FLAG, __PAGES*4096, SUMMIT$3:q
	+	'M',,3
